{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "test_highlights",
      "camelcase": "TestHighlights",
      "title": "TestHighlights",
      "scope": "source.test_highlights",
      "file-types": [
        "test_highlights"
      ],
      "injection-regex": "^test_highlights$",
      "class-name": "TreeSitterTestHighlights"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "TestHighlights grammar for tree-sitter",
    "authors": [
      {
        "name": "German Lashevich",
        "email": "german.lashevich@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-test_highlights"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}